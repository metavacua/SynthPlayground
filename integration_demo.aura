// integration_demo.aura

// 1. Create a simple proof in Intuitionistic Logic (LJ)
// We create an axiom proof of 'A'
let axiom_proof = agent.call_tool("lj.axiom", "A");

// 2. Translate the proof from LJ to Classical Logic (LK)
// This will now return a JSON object representing the proof
let translated_proof_json = agent.call_tool("diagram.translate", axiom_proof, "LJ", "LK");

// 3. Print the translated proof JSON
print(translated_proof_json);
