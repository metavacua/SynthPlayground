# AGENTS.md for calculus: {{ document.title }}
# This file is auto-generated by the calculus_converter.
specVersion: "proof-theoretic-build/v1.0"
sequent:
  # The succedent of this module is the set of all theorems/rules proven in this file.
  succedent:
    {% for element in document.elements -%}
    {% if element[0] == 'prooftree' -%}
    - id: "{{ element[1].rule_name | lower | replace(' ', '_') }}_{{ loop.index }}"
      type: "ProofRule"
      proposition: "Rule '{{ element[1].rule_name }}' with conclusion {{ element[1].conclusion.raw }}"
      provenance:
        conclusion: {{ element[1].conclusion.to_dict() | tojson }}
        hypotheses:
          {% for hypo in element[1].hypotheses -%}
          - {{ hypo.to_dict() | tojson }}
          {% endfor %}
    {% endif -%}
    {% endfor %}