<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/jules/ontologies/2025/10/regular-languages#"
     xml:base="http://www.semanticweb.org/jules/ontologies/2025/10/regular-languages"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/jules/ontologies/2025/10/regular-languages"/>

    <!-- Classes -->
    <owl:Class rdf:about="#RegularLanguage"/>
    <owl:Class rdf:about="#RegularExpression"/>
    <owl:Class rdf:about="#FiniteAutomaton"/>
    <owl:Class rdf:about="#State"/>
    <owl:Class rdf:about="#Transition"/>
    <owl:Class rdf:about="#Alphabet"/>

    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="#definesLanguage">
        <rdfs:domain rdf:resource="#RegularExpression"/>
        <rdfs:range rdf:resource="#RegularLanguage"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#isDefinedBy">
        <owl:inverseOf rdf:resource="#definesLanguage"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#recognizesLanguage">
        <rdfs:domain rdf:resource="#FiniteAutomaton"/>
        <rdfs:range rdf:resource="#RegularLanguage"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#isRecognizedBy">
        <owl:inverseOf rdf:resource="#recognizesLanguage"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasAlphabet">
        <rdfs:domain rdf:resource="#RegularLanguage"/>
        <rdfs:range rdf:resource="#Alphabet"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasState">
        <rdfs:domain rdf:resource="#FiniteAutomaton"/>
        <rdfs:range rdf:resource="#State"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasStartState">
        <rdfs:subPropertyOf rdf:resource="#hasState"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasAcceptState">
        <rdfs:subPropertyOf rdf:resource="#hasState"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasTransition">
        <rdfs:domain rdf:resource="#FiniteAutomaton"/>
        <rdfs:range rdf:resource="#Transition"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasSourceState">
        <rdfs:domain rdf:resource="#Transition"/>
        <rdfs:range rdf:resource="#State"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasTargetState">
        <rdfs:domain rdf:resource="#Transition"/>
        <rdfs:range rdf:resource="#State"/>
    </owl:ObjectProperty>

    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="#hasSymbol">
        <rdfs:domain rdf:resource="#Alphabet"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="#onSymbol">
        <rdfs:domain rdf:resource="#Transition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="#hasRegexString">
        <rdfs:domain rdf:resource="#RegularExpression"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

</rdf:RDF>